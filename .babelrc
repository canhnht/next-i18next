{
  presets: [
    [
      '@babel/env',
      {
        targets: {
          node: 'current',
        },
      },
    ],
    ['@babel/preset-react'],
  ],
  plugins: [
    ['@babel/plugin-proposal-object-rest-spread'],
    [
      'module-resolver',
      {
        root: ['./src'],
      },
    ],
    [
      'add-module-exports',
      {
        addDefaultProperty: true,
      },
    ],
  ],
}
